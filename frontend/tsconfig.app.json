{
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",

    "moduleResolution": "node",
    "jsx": "react-jsx",
    "noEmit": true,

    "types": ["vite/client", "react", "react-dom"],
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}

